{
  "manifest_version": 2,
  "name": "Google Forms Redirect (Multi-account)",
  "version": "1.2",
  "description": "Redirect Google Forms links using the selected Google account index (u/N).",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://docs.google.com/forms/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Select Google Account"
  }
}
